{
  "name": "chat",
  "version": "1.0.0",
  "description": "WebSocket chat app",
  "main": "server.js",
  "scripts": {
    "clientInstall": "cd client && npm install",
    "clientBuild": "cd client && npm run build",
    "clientStart": "cd client && npm run start",
    "installDeps": "npm run clientInstall && npm install",
    "production": "npm run clientBuild && npm run start",
    "start": "node server.js",
    "server": "nodemon server.js",
    "dev": "concurrently \"npm run server\" \"npm run clientStart\""
  },
  "author": "Omkar N",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^7.0.0",
    "moment": "^2.29.1",
    "socket.io": "^4.4.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}
